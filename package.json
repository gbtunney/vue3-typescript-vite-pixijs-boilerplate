{
    "name": "vue3-typescript-vite-pixijs",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --mode localhost",
        "build": "vue-tsc --noEmit && vite build --mode production",
        "preview": "vite preview --mode localhost",
        "fix": "pnpm exec prettier {$npm_package_config_prettier_path}.{$npm_package_config_prettier_extensions} --write --ignore-path ./.gitignore"
    },
    "dependencies": {
        "pixi.js": "^7.2.4",
        "vue": "^3.2.37"
    },
    "config": {
        "prettier": {
            "extensions": "ts,js,cjs,mjs,cts,json,yaml,yml,graphql,md",
            "path": "*,**/*"
        }
    },
    "devDependencies": {
        "@types/node": "^20.1.3",
        "@vitejs/plugin-vue": "^4.2.2",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4",
        "vite": "^4.3.5",
        "vue-tsc": "^1.6.4"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "semi": true,
        "singleQuote": true
    }
}
